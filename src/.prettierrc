{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "overrides": [
    { "files": "*.tsx", "options": { "parser": "typescript" } },
    { "files": "*.jsx", "options": { "parser": "babel" } }
  ]
}
